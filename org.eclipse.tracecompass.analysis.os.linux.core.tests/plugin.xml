<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.linuxtools.tmf.core.analysis">
      <module
            analysis_module="org.eclipse.tracecompass.analysis.os.linux.core.cpuusage.KernelCpuUsageAnalysis"
            automatic="false"
            id="org.eclipse.tracecompass.analysis.os.linux.tests.cpuusage"
            name="CPU Usage Test Analysis">
         <tracetype
               applies="true"
               class="org.eclipse.tracecompass.tmf.tests.stubs.trace.xml.TmfXmlTraceStub">
         </tracetype>
      </module>
      <module
            analysis_module="org.eclipse.tracecompass.analysis.os.linux.core.kernelanalysis.KernelAnalysis"
            automatic="false"
            id="org.eclipse.tracecompass.analysis.os.linux.tests.kernel"
            name="Linux Kernel Test Analysis">
         <tracetype
               applies="true"
               class="org.eclipse.tracecompass.tmf.tests.stubs.trace.xml.TmfXmlTraceStub">
         </tracetype>
      </module>
   </extension>

</plugin>
